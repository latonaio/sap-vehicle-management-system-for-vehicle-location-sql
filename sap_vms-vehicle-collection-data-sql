CREATE TABLE `sap_vms_vehicle_collection_data_sql`
(      
         `VMSVehicleLocation`                     varchar(10) NOT NULL,                
         `VMSVehicleLocation_Text`                varchar(30) DEFAULT NULL,          
         `AddressNumber`                          varchar(10) DEFAULT NULL,     
         `BusinessPartnerName1`                   varchar(40) DEFAULT NULL,    
         `CityName`                               varchar(40) DEFAULT NULL,    
         `PostalCode`                             varchar(10) DEFAULT NULL,    
         `HouseNumber`                            varchar(10) DEFAULT NULL,    
         `StreetName`                             varchar(60) DEFAULT NULL,    
         `Country`                                varchar(3) DEFAULT NULL,    
         `Region`                                 varchar(3) DEFAULT NULL,    
         `LogicalSystem`                          varchar(10) DEFAULT NULL,                                     
    PRIMARY KEY (`VMSVehicleLocation`)
) ENGINE = InnoDB
 DEFAULT CHARSET = utf8mb4;